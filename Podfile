platform :ios, '14.0'
target 'LiveVideo' do
use_frameworks!
# 屏蔽所有第三方框架警告
inhibit_all_warnings!

source 'https://github.com/CocoaPods/Specs.git'

  pod 'Alamofire'

  # 自动布局
  pod 'SnapKit'
  # 加载框
  pod 'Toast-Swift'
  # 键盘
  pod 'IQKeyboardManagerSwift'
  # 扩展
  pod 'JKSwiftExtension'
  
  pod 'HXPhotoPicker'
  
  pod 'RxCocoa'
#  # 避免每个界面都定义disposeBag
  pod 'NSObject+Rx'
  
  # cell复用
  pod 'Reusable'

end

